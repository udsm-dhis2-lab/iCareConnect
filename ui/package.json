{
  "name": "icare-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng serve",
    "build": "ng build",
    "build:prod": "ng build --aot --configuration production",
    "test": "ng test",
    "test:headless": "ng test --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "e2e": "concurrently  -s \"first\" --kill-others \"ng serve\" \"cypress-tags run -e TAGS='@focus'\"",
    "e2e:headed": "concurrently  -s \"first\" --kill-others \"ng serve\" \"cypress-tags run -e TAGS='@focus' --headed\"",
    "e2e:interactive": "concurrently \"ng serve\" \"cypress open\"",
    "clean:reports": "rm -R -f cypress/reports && mkdir cypress/reports && mkdir cypress/reports/mochareports",
    "pretest": "npm run clean:reports",
    "combine-reports": "mochawesome-merge cypress/reports/mocha/*.json > cypress/reports/mochareports/report.json",
    "generate-report": "marge cypress/reports/mochareports/report.json -f report -o cypress/reports/mochareports",
    "posttest": "npm run combine-reports && npm run generate-report",
    "test:e2e": "npm run clean:reports && npm run e2e || npm run posttest",
    "generate-api": "curl 'http://icare.co.tz/openmrs/module/webservices/rest/swagger.json' > ./swagger.json && sed -i 's/parent-uuid/parentUuid/g' swagger.json && swagger-typescript-api -p ./swagger.json -o ./ -n ./src/app/shared/modules/openmrs.ts",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular-material-components/datetime-picker": "^6.0.3",
    "@angular/animations": "~12.1.2",
    "@angular/cdk": "^12.1.2",
    "@angular/common": "~12.1.2",
    "@angular/compiler": "~12.1.2",
    "@angular/core": "~12.1.2",
    "@angular/forms": "^12.1.2",
    "@angular/material": "^12.1.2",
    "@angular/material-moment-adapter": "^13.3.4",
    "@angular/platform-browser": "~12.1.2",
    "@angular/platform-browser-dynamic": "~12.1.2",
    "@angular/router": "~12.1.2",
    "@ckeditor/ckeditor5-angular": "^4.0.0",
    "@kolkov/angular-editor": "^3.0.0-beta.0",
    "@ngrx/effects": "^12.2.0",
    "@ngrx/entity": "^12.2.0",
    "@ngrx/router-store": "^12.2.0",
    "@ngrx/store": "^12.5.1",
    "@ngrx/store-devtools": "^12.2.0",
    "@tinymce/tinymce-angular": "^4.2.4",
    "bootstrap": "^4.5.3",
    "cypress": "^6.8.0",
    "dexie": "^3.2.3",
    "file-saver": "^2.0.5",
    "font-awesome": "^4.7.0",
    "highcharts": "^9.1.2",
    "html-to-pdfmake": "^2.4.15",
    "jspdf": "^2.5.1",
    "material-design-icons": "^3.0.1",
    "moment": "^2.29.4",
    "ngrx-store-freeze": "^0.2.4",
    "ngx-barcode": "^0.3.0",
    "ngx-mat-select-search": "^5.0.0",
    "ngx-print": "^1.2.1",
    "num-words": "^1.2.2",
    "pdfmake": "^0.2.7",
    "quick-pivot": "^2.7.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.1",
    "xlsx": "^0.17.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.1.2",
    "@angular/cli": "^12.1.2",
    "@angular/compiler-cli": "~12.1.2",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.14.6",
    "codelyzer": "^6.0.0",
    "commitizen": "^4.3.0",
    "concurrently": "^5.3.0",
    "cypress-cucumber-preprocessor": "^4.0.0",
    "cypress-multi-reporters": "^1.4.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^4.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "mocha": "^8.2.0",
    "mochawesome": "^6.1.1",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.1.0",
    "protractor": "~7.0.0",
    "swagger-typescript-api": "^9.2.0",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  }
}
